       seriesField: 'name',\r\n        label: {\r\n          // 可手动配置 label 数据标签位置\r\n          position: 'middle', // 'top', 'bottom', 'middle',\r\n          // 配置样式\r\n          style: {\r\n            fill: '#FFFFFF',\r\n            opacity: 100\r\n          },\r\n          formatter(value){\r\n            return value.number+\"项\"\r\n          }\r\n        },\r\n    });\r\n    bar.render();\r\n    },\r\n     moment,\r\n    onChange(dates, dateStrings) {\r\n    },\r\n    selecttab1(){\r\n      this.toptitle=this.tab1;\r\n      this.charts();\r\n      console.log('1')\r\n    },\r\n    selecttab2(){\r\n      console.log('2')\r\n      this.toptitle=this.tab2;\r\n      this.charts();\r\n\r\n    }\r\n  },\r\n  mounted:function(){\r\n    this.charts();\r\n    this.toptitle=this.tab1;\r\n    \r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n  .total{\r\n    text-align: center;\r\n    width: 80%;\r\n    height: 150px;\r\n  }\r\n  .totalnum{\r\n    font-size: 23px;\r\n  }\r\n  #container{\r\n    height: 330px;\r\n    width: 70%;\r\n  }\r\n  .layout-header{\r\n    background: white!important;\r\n    height: 20% !important;\r\n  }\r\n  .layout-content{\r\n    background: white!important;\r\n  }\r\n  .table{\r\n    margin-top: 2% !important;\r\n  }\r\n  .top{\r\n    width:25%;\r\n    height: 330px;\r\n    text-align: center;\r\n  }\r\n  h4{\r\n    margin-top: .5em !important;\r\n    margin-bottom: .5em !important;\r\n    color: rgba(0,0,0,.85) !important;\r\n    font-weight: 500 !important;\r\n  }\r\n  li{\r\n    color: rgba(0,0,0,.85);\r\n    font-size: 14px;\r\n    line-height: 22px;  \r\n  }\r\n  .tab{\r\n    display: flex;\r\n    flex-direction: row;\r\n  }\r\n  .topnum{\r\n    display: inline-block;\r\n    width: 20px;\r\n    height: 20px;\r\n    margin-top: 1.5px;\r\n    margin-right: 16px;\r\n    font-weight: 600;\r\n    font-size: 12px;\r\n    line-height: 20px;\r\n    text-align: center;\r\n    background-color: #314659;\r\n    color: white;\r\n    border-radius